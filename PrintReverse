package LinkedList;

/**
 *
 * @author Vishal
 */
public class PrintReverse 
{
    public static void main(String args[]) 
    {
        Node dup = Node.createList1();
        Node.print(dup);
        System.out.println();
        reversePrint(dup);
    }
    
    public static void reversePrint(Node n)
    {
        if(n == null)
            return;
        
        reversePrint(n.next);
        System.out.println(n.data);
    }
}
